product = str(input())
city = str(input())
qty = int(input())
if city=="Sofia":
    if product=="coffee":
        print(f"You have to pay {0.50*qty}")
    elif product=="water":
        print(f"You have to pay {0.80 * qty}")
    elif product=="beer":
        print(f"Yo have to pay {1.20* qty}")
    elif product=="sweets":
        print (f"You have to pay {1.45 * qty}")
    elif product=="peanuts":
        print (f"You have to pay {1.60 * qty}")
elif city=="Plovdiv":
    if product=="coffee":
        print(f"You have to pay {0.40*qty}")
    elif product=="water":
        print(f"You have to pay {0.70 * qty}")
    elif product=="beer":
        print(f"Yo have to pay {1.15* qty}")
    elif product=="sweets":
        print (f"You have to pay {1.30 * qty}")
    elif product=="peanuts":
        print (f"You have to pay {1.50 * qty}")
elif city=="Varna":
    if product=="coffee":
        print(f"You have to pay {0.45*qty}")
    elif product=="water":
        print(f"You have to pay {0.70 * qty}")
    elif product=="beer":
        print(f"Yo have to pay {1.10* qty}")
    elif product=="sweets":
        print (f"You have to pay {1.35 * qty}")
    elif product=="peanuts":
        print (f"You have to pay {1.55 * qty}")
